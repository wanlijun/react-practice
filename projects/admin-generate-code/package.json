{
	"name": "admin-generate-code",
	"displayName": "admin-generate-code",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:admin-generate-code.helloWorld",
		"onCommand:admin-generate-code.showDate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"menus": {
			"explorer/context": [
				{
					"command": "admin-generate-code.helloWorld",
					"group": "adminCoder@1"
				},
				{
					"command": "admin-generate-code.showDate",
					"group": "adminCoder@1"
				}
			]
		},
		"commands": [
			{
				"command": "admin-generate-code.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "admin-generate-code.showDate",
				"title": "Show DATE"
			}
		],
		"configuration": {
			"title": "GitMagic"
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.64.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3"
	},
	"dependencies": {
		"node-plop": "^0.26.3"
	}
}
